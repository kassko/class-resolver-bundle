{
    "name":"kassko/class-resolver-bundle",
    "authors": [
        {
            "name":"Kassko"
        }
    ],
    "description":"Integrate the class-resolver component to Symfony",
    "keywords":["class", "resolver", "dependency", "injection", "factory"],
    "require":{
        "php":">=5.5.0",
        "symfony/framework-bundle": "^2.5|^3.0|^4.0|^5.0",
        "kassko/class-resolver": "^1.0"
    },
    "require-dev":{
        "phpunit/phpunit": "~4.4"
    },
    "autoload":{
        "psr-4":{
            "Kassko\\Bundle\\ClassResolverBundle\\": "src"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
